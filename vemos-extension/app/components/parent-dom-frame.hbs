<EmberWormhole @destinationElement={{this.parentDomService.container}}>
  <iframe frameBorder="0" {{did-insert this.setIframe}} ...attributes/>
</EmberWormhole>

<div class="hidden">
  <EmberWormhole @destinationElement={{this.iframeDocument}} @renderInPlace={{not this.iframeDocument}}>
    {{yield}}
  </EmberWormhole>
</div>
